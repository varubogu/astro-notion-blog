{
  "name": "astro-notion-blog",
  "type": "module",
  "version": "0.10.0",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro build",
    "build:cached": "bun run cache:fetch && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src",
    "format": "bun x prettier --write .",
    "cache:fetch": "bun scripts/blog-contents-cache.cjs",
    "cache:purge": "nx reset && rm -f tmp/*",
    "_fetch-notion-blocks": "bun scripts/retrieve-block-children.cjs"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@astrojs/react": "^4.4.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@eslint/js": "^9.38.0",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/octicon": "^1.2.16",
    "@iconify/icons-logos": "^1.2.36",
    "@notionhq/client": "^2.3.0",
    "@nrwl/nx-cloud": "^19.1.0",
    "@supercharge/promise-pool": "^3.2.0",
    "@types/async-retry": "^1.4.9",
    "@types/bun": "^1.3.0",
    "@types/katex": "^0.16.7",
    "@types/metascraper": "^5.41.0",
    "@types/metascraper-description": "^5.42.0",
    "@types/metascraper-image": "^5.42.0",
    "@types/metascraper-title": "^5.42.0",
    "@types/prismjs": "^1.26.5",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@typescript-eslint/typescript-estree": "^8.46.2",
    "astro": "^5.14.7",
    "astro-eslint-parser": "^1.2.2",
    "astro-icon": "^1.1.5",
    "async-retry": "^1.3.3",
    "axios": "^1.12.2",
    "cli-progress": "^3.12.0",
    "eslint": "^9.38.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "exif-be-gone": "^1.5.1",
    "globals": "^16.4.0",
    "katex": "^0.16.25",
    "mermaid": "^11.12.0",
    "metascraper": "^5.49.5",
    "metascraper-description": "^5.49.5",
    "metascraper-image": "^5.49.5",
    "metascraper-title": "^5.49.5",
    "nx": "^20.8.2",
    "prismjs": "^1.30.0",
    "sharp": "^0.34.4",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.46.2"
  },
  "nx": {
    "targets": {
      "_fetch-notion-blocks": {
        "inputs": [
          "!{projectRoot}/**/*",
          "!{projectRoot}/**/.*",
          "!{projectRoot}/**/.*/**/*",
          {
            "env": "DATABASE_ID"
          }
        ],
        "outputs": [
          "{projectRoot}/tmp"
        ]
      }
    },
    "includedScripts": [
      "_fetch-notion-blocks"
    ]
  }
}
